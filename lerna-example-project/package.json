{
  "name": "@dtn/web-ui",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">= 10 < 11",
    "npm": ">= 6 < 7"
  },
  "scripts": {
    "start": "lerna run start --parallel",
    "tsc": "lerna run tsc",
    "build": "lerna run build",
    "build:docs": "node documentation/html/index.js --output-path docs-site --logs",
    "build:preview-pages": "node documentation/preview/html/index.js",
    "build:preview": "npm run build:preview-pages && bash ./documentation/preview/shell-scripts/dist-extractor.sh",
    "test": "npm run-script prepush",
    "format": "prettier --config .prettierrc.js",
    "lint": "lerna run lint --stream",
    "postinstall": "lerna bootstrap --hoist",
    "publish-all": "lerna publish --conventional-commits --changelog-preset conventional-bitbucket --dry-run"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@commitlint/config-lerna-scopes": "^7.5.1",
    "@dtn/eslint-config": "^0.2.3",
    "@dtn/prettier-config": "^0.2.2",
    "@types/d3-dsv": "^1.0.33",
    "@types/d3-format": "^1.3.0",
    "@types/d3-time-format": "^2.1.0",
    "@types/i18next": "^8.4.3",
    "@types/lodash-es": "^4.17.3",
    "@types/mapbox-gl": "0.48.0",
    "@types/moment-timezone": "^0.5.4",
    "@types/node": "^9.6.5",
    "@types/react": "^16.8.7",
    "@types/react-dom": "^16.8.2",
    "@types/react-redux": "^5.0.16",
    "@types/scriptjs": "0.0.2",
    "commitlint": "^7.0.0",
    "d3-dsv": "^1.0.8",
    "d3-format": "^1.3.0",
    "d3-time-format": "^2.1.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "fs-extra": "^7.0.0",
    "git-semver-tags": "^2.0.2",
    "husky": "^1.3.1",
    "idempotent-babel-polyfill": "^7.0.0",
    "jest": "^24.0.0",
    "lerna": "^3.13.1",
    "lint-staged": "^8.1.5",
    "lodash": "^4.17.10",
    "lodash-es": "^4.17.10",
    "mapbox-gl": "0.53.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.22.1",
    "moment-timezone": "^0.5.15",
    "prettier": "~1.17.0",
    "react": "^16.8.4",
    "react-day-picker": "^7.2.4",
    "react-dom": "^16.8.4",
    "react-redux": "^5.0.7",
    "react-stockcharts": "^0.7.7",
    "recast": "^0.15.2",
    "redux": "^4.0.0",
    "redux-observable": "^1.0.0",
    "reselect": "^3.0.1",
    "rimraf": "^2.6.2",
    "rxjs": "^6.4.0",
    "scriptjs": "^2.5.9",
    "styled-components": "^3.3.0",
    "tslint": "^5.9.1",
