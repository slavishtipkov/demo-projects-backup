{
  "compilerOptions": {
    "declaration": true,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es5",
    "lib": ["esnext", "dom"],
    "forceConsistentCasingInFileNames": true,
    "allowUnreachableCode": false,
    "noFallthroughCasesInSwitch": true,
    "alwaysStrict": true,
    "strict": true,
    "jsx": "react",
    "sourceMap": true
  }
}
